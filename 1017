using System;
using System.Globalization;

namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            int T, Vm;
            double Dist, L;
            T = int.Parse(Console.ReadLine());
            Vm = int.Parse(Console.ReadLine());
            Dist = Vm * T;
            L = Dist / 12;
            Console.WriteLine(L.ToString("F3",CultureInfo.InvariantCulture));
        }
    }
}
